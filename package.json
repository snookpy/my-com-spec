{
  "name": "my-com-spec",
  "version": "0.1.0",
  "private": true,
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "electron-is-dev": "^1.1.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "systeminformation": "^4.15.3",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "PORT=5555 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:5555 && electron .\"",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder build -mwl -c.extraMetadata.main=build/main.js"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "public/main.js"
    ],
    "publish": {
      "provider": "github",
      "repo": "my-com-spec",
      "owner": "Nookie"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.2",
    "concurrently": "^5.0.0",
    "electron": "^18.3.7",
    "electron-builder": "^21.2.0",
    "wait-on": "^3.3.0"
  }
}
